<Application x:Class="GeekGrapher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GeekGrapher"
              xmlns:sys="clr-namespace:System;assembly=mscorlib"
             StartupUri="StartWindow/StartWindow.xaml"
             Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Assets/Fonts/FontAwesome5/FontAwesome5Dictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <sys:String x:Key="Authors">../../Assets/AboutUs/Authors.xml</sys:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
